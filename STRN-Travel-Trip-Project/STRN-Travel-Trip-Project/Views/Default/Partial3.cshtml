@using STRN_Travel_Trip_Project.Models.Siniflar
@model List<Blog>

<div class="padding">
    <h4> Popular 10 Blog </h4>

    <ul class="list list-pad">
       @{ int i = 0;}
        @foreach (var x in Model)
        {


            <li class="list-bg"><span>@{i++;} @i.</span><a href="~/Blog/BlogDetail/@x.Id">@x.Baslik</a></li>
        }
    </ul>
    <a href="~/Blog/Index" class="link1">See all Blog</a>
</div>
