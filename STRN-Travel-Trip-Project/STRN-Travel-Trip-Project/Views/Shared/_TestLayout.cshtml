            <div class="logo"><a href="index.html"><img src="~/web/images/logo.png" alt="" /></a></div>
            <div class="con-right">
                <div class="grid1-l-img">
                    <img src="~/web/images/contact.png" alt="">
                </div>
                <div class="grid1-l-desc">
                    <p>Contact :05392589878</p>
                </div>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
    </div>
    <div class="header-bottom">
        <div class="wrap">
            <ul id="nav">
                <li><a href="/Default/Index">Home</a></li>
                <li><a class="hsubs" href="/About/Index">About</a></li>
                <li><a class="hsubs" href="/Blog/Index">Blog</a></li>
                <li><a href="~/Contact/Index">Contact</a></li>
            </ul>
        </div>
    </div>

    <div class"content">
        @RenderBody()
    </div>
    <div class="footer-bottom">
        <div class="wrap">
            <div class="copy">
                <p>&copy; 2022 StrnSoft. All Rights Reserved | Design by <a href="www.ruveydaakcinar.com">StrnSoft</a></p>
            </div>
